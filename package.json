{
  "name": "tt-for-shm",
  "displayName": "TT for SHM",
  "description": "VS Code support for SHM Template Toolkit syntax {{..}}",
  "author":{
    "name": "Roman Trifanov"
  },
  "repository": "https://github.com/RomanTrifanov/VSCode-support-SHM-template",
  "homepage": "https://github.com/RomanTrifanov/VSCode-support-SHM-template/blob/main/README.md",
  "version": "1.0.0",
  "publisher": "romant38",
  "license": "MIT",
  "icon": "./images/logo.png",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tt2",
      "aliases": ["TT2", "tt2"],
      "extensions": [".tt",".tpl"],
      "configuration": "./language-configuration.json"
    }],
    "keywords": [
      "shm",
      "tt",
      "perl",
      "template",
      "template toolkit"
    ],
    "grammars": [{
      "language": "tt2",
      "scopeName": "source.tt2",
      "path": "./syntaxes/tt2.tmLanguage"
    }],
    "snippets": [{
      "language": "tt2",
      "path": "./snippets/snippets.json"
    }]
  }
}
